{
  "name": "NAVs-AAD-Example-APP",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|jpg|png)$": "<rootDir>/empty-module.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|lodash-es|react-redux)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "version": "1.0.0",
  "description": "Example APP for NAVs Azure AD authorization flow",
  "main": "api/src/startApp.js",
  "dependencies": {
    "better-npm-run": "^0.1.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "express": "^4.16.3",
    "helmet": "^3.14.0",
    "morgan": "^1.9.0",
    "nock": "^10.0.1",
    "passport": "^0.4.0",
    "passport-azure-ad": "^4.0.0",
    "pretty-quick": "^1.6.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "expect": "^23.5.0",
    "history": "^4.7.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "less": "^3.8.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2"
  },
  "scripts": {
    "test": "jest",
    "start": "better-npm-run start",
    "api": "better-npm-run dev-api"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon api/src/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev-api": {
      "command": "nodemon api/src/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "AAD",
    "AzureAD"
  ],
  "author": "@navikt/aura",
  "license": "MIT"
}
